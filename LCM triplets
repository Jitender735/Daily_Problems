class Solution {
  public:
    long long lcmTriplets(long long n) {
        // code here
        
        if(n<=2)
        return n;
        if(n%2!=0)
        return n*(n-1)*(n-2);// 3
        if(n%3==0)
        return (n-1)*(n-2)*(n-3);//6
        if(n%2==0)
        return n*(n-1)*(n-3);//8
        
    }
};

//DIFFERENT CASES ARE THERE 
